#ifndef GAMEBOY_VERSION_H
#define GAMEBOY_VERSION_H

#include <string_view>

namespace gameboy::version {

[[maybe_unused]] constexpr auto major = @gameboycore_VERSION_MAJOR@;
[[maybe_unused]] constexpr auto minor = @gameboycore_VERSION_MINOR@;
[[maybe_unused]] constexpr auto patch = @gameboycore_VERSION_PATCH@;

[[maybe_unused]] constexpr std::string_view version = "@gameboycore_VERSION_MAJOR@.@gameboycore_VERSION_MINOR@.@gameboycore_VERSION_PATCH@";

} // namespace gameboy::version

#endif //GAMEBOY_VERSION_H
